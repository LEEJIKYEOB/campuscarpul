<resources>


    <style name="DialogccGori" parent="@android:style/Theme.Holo.Light.Dialog.NoActionBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:listChoiceIndicatorSingle">@android:color/black</item>
        <item name="android:windowTitleStyle">@style/DialogccTitle</item>
    </style>

    <style name="DialogccTitle">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="datePickerTheme" parent="@android:style/Widget.Holo.DatePicker">
        <item name="android:divider">@color/hintcolor</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <item name="windowActionBar">false</item>
        <item name="colorAccent">@color/lightblue_500</item>
        <item name="colorPrimary">@color/lightblue_500</item>
        <item name="colorPrimaryDark">@android:color/holo_orange_dark</item>
        <item name="colorControlHighlight">@android:color/holo_red_dark</item>
        <item name="colorControlNormal">@color/white</item>
        <item name="android:textColorPrimary">@android:color/white</item>
    </style>



</resources>
